{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/contact",
      "dest": "main.py",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}